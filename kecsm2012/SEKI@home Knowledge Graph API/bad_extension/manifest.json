{
  "name": "Google clicker",
  "version": "0.1",
  "manifest_version": 2,
  "description": "bad extension",
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "js": [
        "js/jquery.js",
        "js/contentscript.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "offline_enabled": false,
  "permissions": [
    "https://www.google.com/*"
  ]
}