{
  "name": "SEKI@home",
  "version": "0.1",
  "manifest_version": 2,
  "description": "SEKI@home, or Crowdsourcing a Google Knowledge Graph API",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "default_locale": "en_US",
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "js": [
        "js/jquery.js",
        "js/URI.min.js",
        "js/contentscript.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "offline_enabled": false,
  "permissions": [
    "http://openknowledgegraph.org/*",
    "http://localhost/*",
    "https://www.google.com/*"
  ]
}